{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=5432;Database=EShopDb;User Id=postgres;Password=postgres",
    "Redis": "localhost:6379"
  },
  // "Logging": {
  //   "LogLevel": {
  //     "Default": "Information",
  //     "Microsoft.AspNetCore": "Warning"
  //   }
  // },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Properties": {
      "Application": "EShop Asp .Net Core App",
      "Environment": "Development"
    }
  },
  "AllowedHosts": "*"
}
